<?xml version="1.0" encoding="ISO-8859-1"?>
<splitpane divider="120">
	<tree action="loadDraft(this, preview)">
		<node text="Components">
			<node text="TabbedPane"
					property="ui=tabbedpane.xml"
					property="logic=thinlet.drafts.TabbedPane" />
		</node>
		<node text="MDI"
				property="ui=mdi.xml"
				property="logic=thinlet.drafts.MDI" />
		<node text="Style"
				property="ui=style.xml"
				property="logic=thinlet.drafts.Style" />
		<node text="System" >
			<node text="Properties"
					property="ui=systemproperties.xml"
					property="logic=thinlet.drafts.SystemProperties" />
		</node>
	</tree>
	<panel name="preview" border="true" top="1" left="1" bottom="1" right="1" />
</splitpane>
