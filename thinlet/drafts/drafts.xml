<?xml version="1.0" encoding="ISO-8859-1"?>
<splitpane name="content" divider="120">
	<tree action="loadDraft(content, this)">
		<node text="Components">
			<node text="Tabbed pane"
					property="ui=tabbedpane.xml"
					property="logic=thinlet.drafts.TabbedPane" />
			<node text="MDI"
					property="ui=mdi.xml"
					property="logic=thinlet.drafts.MDI" />
			<node text="Class explorer"
					property="ui=classexplorer.xml"
					property="logic=thinlet.drafts.ClassExplorer" />
		</node>
		<node text="Style"
				property="ui=style.xml"
				property="logic=thinlet.drafts.Style" />
		<node text="System" >
			<node text="Properties"
					property="ui=systemproperties.xml"
					property="logic=thinlet.drafts.SystemProperties" />
			<node text="Colors"
					property="ui=systemcolors.xml"
					property="logic=thinlet.drafts.SystemColors" />
			<node text="Windows"
					property="ui=desktopproperties.xml"
					property="logic=thinlet.drafts.DesktopProperties" />
		</node>
	</tree>
	<panel border="true" />
</splitpane>
